{
  "name": "wraptor-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"make-agent dev -p 3000\"",
    "dev-testnet": "concurrently \"next dev --turbopack\" \"make-agent dev -p 3000 -t\"",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "next lint --fix && prettier --write '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@bitteprotocol/agent-sdk": "^0.0.8",
    "next": "15.0.1",
    "swagger-ui-react": "5.12.0",
    "viem": "^2.22.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/swagger-ui-react": "^4.18.3",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.1",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "make-agent": "^0.0.24",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
