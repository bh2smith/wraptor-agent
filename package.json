{
  "name": "wraptor-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"make-agent dev\"",
    "dev-testnet": "concurrently \"next dev\" \"make-agent dev -t\"",
    "build:next": "next build",
    "build": "next build",
    "deploy": "make-agent deploy -u https://wraptor-agent.vercel.app/",
    "start": "next start",
    "lint": "next lint && prettier --check '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "next lint --fix && prettier --write '{src,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.2.7",
    "next": "^15.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "redoc": "^2.5.0",
    "viem": "^2.34.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "concurrently": "^9.2.0",
    "eslint-config-next": "^15.5.0",
    "jest": "^30.0.5",
    "make-agent": "0.3.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
