{
  "name": "wraptor-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev --turbopack\" \"make-agent dev -p 3000\"",
    "dev-testnet": "concurrently \"next dev --turbopack\" \"make-agent dev -p 3000 -t\"",
    "build:next": "next build",
    "build": "next build",
    "deploy": "next build && make-agent deploy -u https://wraptor-agent.vercel.app/",
    "start": "next start",
    "lint": "next lint && prettier --check '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "next lint --fix && prettier --write '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.1.11",
    "near-safe": "^0.9.14",
    "next": "^15.3.4",
    "redoc": "^2.5.0",
    "viem": "^2.31.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.32",
    "@types/react": "^19.1.8",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint-config-next": "^15.3.4",
    "jest": "^29.7.0",
    "make-agent": "0.2.11",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
