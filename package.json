{
  "name": "wraptor-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"make-agent dev\"",
    "dev-testnet": "concurrently \"next dev\" \"make-agent dev -t\"",
    "build:next": "next build",
    "build": "next build",
    "deploy": "make-agent deploy -u https://wraptor-agent.vercel.app/",
    "start": "next start",
    "lint": "next lint && prettier --check '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "fmt": "next lint --fix && prettier --write '{src/app,tests}/**/*.{js,jsx,ts,tsx}'",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@bitte-ai/agent-sdk": "^0.2.1",
    "near-safe": "^0.10.0",
    "next": "^15.3.4",
    "redoc": "^2.5.0",
    "viem": "^2.31.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "concurrently": "^9.2.0",
    "dotenv": "^16.5.0",
    "eslint-config-next": "^15.3.4",
    "jest": "^30.0.3",
    "make-agent": "0.3.2",
    "prettier": "^3.6.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
